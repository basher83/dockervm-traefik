# Environment Variables for dockervm-traefik Stack
# Copy this file to .env and customize for your environment

# Domain Configuration
DOMAIN=lab.spaceships.work
LETSENCRYPT_EMAIL=admin@lab.spaceships.work

# Traefik Configuration
TRAEFIK_VERSION=3.3
TZ=UTC

# Port Configuration
TRAEFIK_HTTP_PORT=8081
TRAEFIK_HTTPS_PORT=8443
TRAEFIK_DASHBOARD_PORT=8082

# Docker Socket Proxy
SOCKET_PROXY_SUBNET=10.91.0.0/24

# SSL Configuration
SSL_ENABLED=true

# Logging
LOG_LEVEL=INFO

# Authentication (if needed)
# BASIC_AUTH_USERS=admin:$2y$10$2b2cu2Fw2X2hCjPq8XzLve3hKDgWbgRAA4XqLs9U1LW.e8YKHpvXe

# Cloudflare DNS Challenge (if using)
# CF_DNS_API_TOKEN=your_cloudflare_token

# Resource Limits (optional)
TRAEFIK_CPU_LIMIT=1
TRAEFIK_MEMORY_LIMIT=512M

# Monitoring (optional)
METRICS_ENABLED=false
METRICS_PORT=8083
